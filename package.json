{
  "name": "vestuviu-backend",
  "version": "1.0.0",
  "private": true,
  "description": "RSVP backend for Vestuviu kvietimas (Express + Prisma + Postgres)",
  "main": "src/server.js",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "node --watch src/server.js",
    "start": "node src/server.js",
